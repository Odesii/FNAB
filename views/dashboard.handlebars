<h1>My Dashboard</h1>
{{#each posts}}
    <div class="post">
        <h2>{{this.title}}</h2>
        <p>{{this.content}}</p>
        <button><a href="/posts/edit/{{this.id}}">Edit</a></button>
        <button><a href="/posts/delete/{{this.id}}">Delete</a></button>
    </div>
{{/each}}
<button><a href="/posts/new">New Post</a></button>
